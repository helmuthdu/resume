{
  "name": "resume",
  "homepage": "http://helmuthdu.github.io/resume",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "test:coverage": "react-app-rewired test --env=jsdom --coverage",
    "test:debug": "react-app-rewired --inspect-brk test --runInBand",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "flow": "flow",
    "build-storybook": "build-storybook -s public",
    "storybook": "start-storybook -p 9009 -s public",
    "coveralls": "react-app-rewired test --env=jsdom --coverage --coverageReporters=text-lcov | coveralls"
  },
  "dependencies": {
    "anchorate": "^1.2.3",
    "aphrodite": "^2.2.3",
    "connected-react-router": "^4.5.0",
    "history": "^4.7.2",
    "lodash": "^4.17.11",
    "prismjs": "^1.15.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-google-maps": "^9.4.5",
    "react-helmet": "^5.2.0",
    "react-images": "^0.5.19",
    "react-redux": "^5.1.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-thunk": "^2.3.0",
    "semantic-ui-css": "2.4.1",
    "semantic-ui-react": "^0.83.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.11",
    "@storybook/addon-knobs": "^3.4.11",
    "@storybook/addon-links": "^3.4.11",
    "@storybook/addons": "^3.4.11",
    "@storybook/react": "^3.4.11",
    "babel-loader": "8.0.4",
    "coveralls": "^3.0.2",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-react-app": "^3.0.4",
    "eslint-plugin-flowtype": "^3.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.84.0",
    "gh-pages": "^2.0.1",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3",
    "react-app-rewire-eslint": "^0.2.3",
    "react-app-rewire-hot-loader": "^1.0.3",
    "react-app-rewire-styled-components": "^3.0.2",
    "react-app-rewired": "^1.6.2",
    "react-hot-loader": "^4.3.11",
    "react-scripts": "^2.0.5",
    "react-test-renderer": "^16.6.0",
    "redux-mock-store": "^1.5.3",
    "storybook-readme": "^4.0.2",
    "webpack": "4.19.1"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.stories.js",
      "!src/index.js",
      "!src/serviceWorker.js",
      "!src/setupTests.js",
      "!node_modules/"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
