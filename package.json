    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx}": [
      "xo --fix",
      "git add"
    ],
    "*.md": [
      "remark -o",
      "git add"
    ],
    "*.css": [
      "stylelint"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  }
